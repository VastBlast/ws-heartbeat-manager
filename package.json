{
  "name": "ws-heartbeat-manager",
  "version": "0.1.1",
  "description": "Lightweight WebSocket heartbeat manager for ws servers.",
  "scripts": {
    "build": "tsdown",
    "build:watch": "tsdown --watch",
    "lint": "oxlint --type-aware",
    "lint:fix": "oxlint --fix --type-aware",
    "check:types": "tsgo --noEmit",
    "check": "npm run lint && npm run check:types",
    "test": "vitest",
    "prepublishOnly": "npm test -- --run && npm run check && npm run build"
  },
  "keywords": [
    "websocket",
    "ws",
    "heartbeat",
    "ping",
    "pong"
  ],
  "author": "VastBlast",
  "license": "MIT",
  "files": [
    "dist/"
  ],
  "sideEffects": false,
  "peerDependencies": {
    "ws": "^8.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.3",
    "@types/ws": "^8.18.1",
    "@typescript/native-preview": "^7.0.0-dev.20251223.1",
    "oxlint": "^1.35.0",
    "oxlint-tsgolint": "^0.14.2",
    "tsdown": "^0.20.3",
    "typescript": "^5.8.3",
    "vitest": "^4.0.16"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  }
}
